@model StudentProject.Models.student
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</head>
<body>
    <div class="container">
        <nav class="navbar navbar-expand-lg " style="background-color: #4d99b3 ">
            <div class="container-fluid">

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="~/Student/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " aria-current="page" href="~/Student/About">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " aria-current="page" href="~/Student/Contact">Contact</a>
                        </li>
                    </ul>
                   
                </div>
            </div>
        </nav>
        <div>
            <br />
            <h1>Student Details</h1>
            <br />
            <table class="table table-primary table-bordered ">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>NAME</th>
                        <th>DISCRIPTION</th>
                        <th>CITY</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>@Model.Id</td>
                        <td>@Model.Name</td>
                        <td>@Model.Description</td>
                        <td>@Model.city</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>

</body>
</html>
